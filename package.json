{
  "name": "dale",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "scripts": {
    "clean": "rimraf .build .serverless .nyc_output coverage",
    "predeploy": "npm run clean",
    "deploy": "SET SLS_DEBUG=* && serverless deploy --aws-profile personal --dbConfig AWS",
    "start": "serverless offline -s dev --dbConfig LOCAL",
    "test": "echo \"Error: no test specified\" && exit 1",
    "invoke": "serverless invoke local --function hello --log"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "mysql": "^2.18.1",
    "typeorm": "^0.2.25",
    "typeorm-aurora-data-api-driver": "^1.3.1",
    "typeorm-naming-strategies": "^1.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "rimraf": "^3.0.2",
    "serverless": "^1.71.3",
    "serverless-offline": "^6.3.0",
    "serverless-plugin-typescript": "^1.1.9",
    "typescript": "^3.9.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
